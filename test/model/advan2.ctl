$PROBLEM Start model
$INPUT ID TIME AMT DV WT HT BMI AGE SEX SCR ALB LDH ALP ALT TCHOL CRCL
$DATA run.csv IGNORE=@
$SUBROUTINES ADVAN2 TRANS2
$ABBR REPLACE ETA_CL=ETA(1)
$ABBR REPLACE ETA_VC=ETA(2)
$ABBR REPLACE ETA_MAT=ETA(3)
$PK
WT_MEDIAN = 70.2000000000000
MAT = THETA(3)*EXP(ETA_MAT)
CL = THETA(1)*EXP(ETA_CL)
VC = THETA(2)*EXP(ETA_VC)
VCWT = (WT/WT_MEDIAN)**THETA(4)
VC = VC*VCWT
KA = 1/MAT
V = VC
$ERROR
IPRED = A(2)/VC
IF (IPRED.EQ.0) THEN
    IPREDADJ = 2.22500000000000E-16
ELSE
    IPREDADJ = IPRED
END IF
Y = IPREDADJ * EXP(EPS(1))
$THETA  (0,0.5) ; POP_CL
$THETA  (0,0.2) ; POP_VC
$THETA  (0,0.1) ; POP_MAT
$THETA  (-100.0,1.0,100000.0) ; POP_VCWT
$OMEGA BLOCK(2)
0.04000000000000001	; IIV_CL
0.0	; IIV_CL_IIV_VC
0.04000000000000001	; IIV_VC
$OMEGA  0.04000000000000001 ; IIV_MAT
$SIGMA  0.16000000000000003 ; sigma
$EST METHOD=COND INTER MAXEVAL=99999 AUTO=1 PHITYPE=1 FNLETA=0
